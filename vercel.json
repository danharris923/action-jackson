{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "data/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/data/deals.json",
      "dest": "/data/deals.json"
    },
    {
      "src": "/((?!data/).)*",
      "dest": "/index.html"
    }
  ]
}